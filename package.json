{
  "name": "@lyvly/eslint-config-lyvly",
  "version": "2.0.0",
  "description": "Lyvly standard ESLint configs",
  "main": "src/index.js",
  "files": [
    "src/*"
  ],
  "scripts": {
    "------ LINTING ------": "",
    "lint": "eslint --ext .js .",
    "------ TEST ------": "",
    "test": "jest",
    "------ BUILD/RELEASE ------": "",
    "release:major": "npm version major && npm publish",
    "release:minor": "npm version minor && npm publish",
    "release:patch": "npm version patch && npm publish",
    "------ NPM Scripts ------": "",
    "preversion": "yarn lint && yarn test",
    "postversion": "git push --follow-tags",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint && yarn test",
      "pre-commit": "yarn lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyvly-living/eslint-config-lyvly.git"
  },
  "author": "Ray Parkar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyvly-living/eslint-config-lyvly/issues"
  },
  "homepage": "https://github.com/lyvly-living/eslint-config-lyvly#readme",
  "dependencies": {
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  },
  "peerDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0 || ^6.7.2",
    "eslint-plugin-import": "^2.20.1"
  },
  "devDependencies": {
    "auto-changelog": "^1.16.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "eslint": "^5.16.0 || ^6.7.2",
    "eslint-find-rules": "^3.4.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0"
  }
}
